{
	"name": "library_checkout",
	"version": "1.0.0",
	"description": "library checkout gui",
	"main": "",
	"scripts": {
		"install-server": "npm install --prefix server",
		"i-server": "npm install --prefix server",
		"update-server": "npm update --prefix server",
		"uninstall-server": "npm uninstall --prefix server",
		"install-client": "npm install --prefix client",
		"i-client": "npm install --prefix client",
		"update-client": "npm update --prefix client",
		"uninstall-client": "npm uninstall --prefix client",
		"install-all": "npm i && npm run i-server && npm run i-client",
		"i-all": "npm i && npm run i-server && npm run i-client",
		"update-all": "npm update && npm run update-server && npm run update-client",
		"uninstall-all": "npm uninstall && npm run uninstall-server && npm run uninstall-client",
		"server": "npm run server --prefix server",
		"client": "npm start --prefix client",
		"dev": "concurrently 'npm run server' 'npm run client'",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "Zachary Williams <zak.williams2287@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^8.2.0"
	},
	"devDependencies": {
		"concurrently": "^5.1.0"
	}
}
